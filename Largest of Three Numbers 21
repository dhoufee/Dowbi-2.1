import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        /* Enter your code here. Read input from STDIN. Print output to STDOUT. Your class should be named Solution. */
        Scanner s=new Scanner(System.in);
        int A,B,C;
        A=s.nextInt();
        B=s.nextInt();
        C=s.nextInt();
        if(A>B){
            if(A>C){
            
            System.out.print(A);
            }
        }
        else if(B>A){
                if(B>C){
           
                System.out.print(B);
            }
        }
            else{
                System.out.print(C);
            }
        }
        }
    
